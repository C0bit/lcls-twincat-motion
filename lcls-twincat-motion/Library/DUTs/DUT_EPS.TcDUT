<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.6">
  <DUT Name="DUT_EPS" Id="{25af1f1b-2fda-458f-bcd3-ff2a10606676}">
    <Declaration><![CDATA[TYPE DUT_EPS :
STRUCT
	// Contains EPS flags
    {attribute 'pytmc' := '
        pv: EPS:nFlags
        io: io
        field: DESC Contains EPS flags
    '}
	nFlags: UDINT:=0;
    
    // Desciption of values nFlags contains
	{attribute 'pytmc' := '
		pv:EPS:sDesc
		io: io
		field: DESC of nFlag variable
	'}
	sFlagDesc:  STRING;
	
	// Number of bits used in nFlags
	{attribute 'pytmc' := '
		pv: EPS:nNBitUsed
		io: io
		field: DESC number of bits used
	'}
	nBitsUsed: BYTE;
	
	
	// Name to use for log messages.
    {attribute 'pytmc' := '
        pv: EPS:sMessage
        io: i
        field: DESC Message from EPS to usr
    '}
    sMessage: STRING;
END_STRUCT
END_TYPE
]]></Declaration>
  </DUT>
</TcPlcObject>